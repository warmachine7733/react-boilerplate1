{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "1st boiler-plate from free code camp",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build-babel": "npm run build-babel-server && npm run build-babel-client",
    "build-babel-server": "babel src/server --out-dir ./dist",
    "build-babel-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/warmachine7733/react-boilerplate1.git"
  },
  "author": "prateek jena",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/warmachine7733/react-boilerplate1/issues"
  },
  "homepage": "https://github.com/warmachine7733/react-boilerplate1#readme",
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "webpack": "^3.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "webpack-node-externals": "^1.7.2",
    "html-webpack-plugin": "~3.2.0"
  }
}
